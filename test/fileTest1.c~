#include "syscall.h"

int main(void)
{
	int result = Create("file1");
	if (result != 1) Msg("Creating file failed!");
	
	result = OpenAFile("file1");
	if(result < 0) {
		Msg("Opening file failed!");
	}
    
    result = CloseFile();
	if (result != 1) Msg("Closing file failed!");
	
	Msg("Success :)");
}
